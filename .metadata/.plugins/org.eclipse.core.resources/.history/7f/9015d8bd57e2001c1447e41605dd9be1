package com.hospital;

public class Patients {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//upcasting
		
		Hospital timing=new Hospital();
		
		
		timing.working_time(9);
		
		System.out.println(timing.leaving_time(11));
		
		
		timing=new Doctor();
		timing.working_time(11);
		
		System.out.println(timing.leaving_time(8));
		
//	System.out.println(timing.working_time(11));

	}

}
class Hospital {
	static String hospital_Name;
	int Employee_id;
	
	
	public void working_time(int startingTime) {
		System.out.print("the Hospital Start at "+startingTime);
	}
	
	public int leaving_time(int leavingTime) {
		System.out.print(" we can leave at time is ");
		
		return leavingTime;
		
	}
	
}

class Doctor extends Hospital{
	public void working_time(int startingTime) {
		System.out.print("the hospital doctor come at "+startingTime);
	}
	
	public int leaving_time(int leavingTime) {
		System.out.print(" doctor will  leaves at time ");
		
		return leavingTime;
		
	}
}

class nurse extends Hospital{
	public void workingTime(int startingTime) {
		System.out.println("in the hospital nurse comes time "+startingTime);
	}
	
	public int leaving_time(int leavingTime) {
		System.out.println("nurse leaves at  time ");
		
		return leavingTime;
	}
}

class management extends Hospital{
	public void workingTime(int startingTime) {
		System.out.print("in the hospital management Team comes at time "+startingTime);
	}
	
	public int leaving_time(int leavingTime) {
		System.out.println(" management team can leaving time ");
		return leavingTime;
	}
	
}